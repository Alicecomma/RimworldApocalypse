<NameMakerDefs>
	<RulePackDef>
		<defName>NamerMountain</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[prename] [partialname]</li>
				<li>partialname->[title] [name]</li>
				<li>partialname->[name] [mountain]</li>
				<li>partialname->[name] [mountains]</li>
